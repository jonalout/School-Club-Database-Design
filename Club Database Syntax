/* CREATE TABLES SYNTAX */

USE VP;

CREATE TABLE STUDENTMAJOR ( 
	MajorID Int NOT NULL, 
	MajorName Char(50) NOT NULL,
	CONSTRAINT STUDENTMAJOR_PK PRIMARY KEY(MajorID)
	);

CREATE TABLE SPONSOR ( 
	SponsorID Int NOT NULL, 
	SponsorFN Char(50) NOT NULL, 
	SponsorLN Char(50) NOT NULL, 
	CompanyName Char(100) NOT NULL,
	Email Char(100) NOT NULL,
	PhoneNumber Char(20) NOT NULL, 
	CONSTRAINT SPONSOR_PK PRIMARY KEY(SponsorID)
	);

CREATE TABLE FUNDRAISERTYPE ( 
	FundTypeID Int NOT NULL, 
	FundName Char(50) NOT NULL, 
	Description Char(100) NOT NULL,
	CONSTRAINT FUNDRAISERTYPE_PK PRIMARY KEY(FundTypeID)
	);

CREATE TABLE STUDENT ( 
	StudentID Int NOT NULL,
	StudentFN Char(50) NOT NULL, 
	StudentLN Char(50) NOT NULL, 
	Email Char(100) NOT NULL,
	Age Int NOT NULL,
	MajorID Int NOT NULL, 
	PhoneNumber Char(20) NOT NULL,
	CONSTRAINT STUDENT_PK PRIMARY KEY(StudentID),
	CONSTRAINT STUD_MAJORID_FK FOREIGN KEY(MajorID) REFERENCES STUDENTMAJOR (MajorID)
	);

CREATE TABLE CLADVISOR ( 
	AdvisorID Int NOT NULL,
	AdvisorFN Char(50) NOT NULL, 
	AdvisorLN Char(50) NOT NULL, 
	Email Char(100) NOT NULL,
	CONSTRAINT CLADVISOR_PK PRIMARY KEY(AdvisorID) 
	);

CREATE TABLE CLACTIVITIES ( 
	ActivityID Int NOT NULL,
	Description Char(100) NOT NULL,
	Date Date NOT NULL,
	Participants Int NOT NULL,
	CONSTRAINT CLACTIVITIES_PK PRIMARY KEY(ActivityID) 
	);

CREATE TABLE CLUB ( 
	ClubID Int NOT NULL,
	AdvisorID Int NOT NULL, 
	ClubName Char(100) NOT NULL, 
	Category Char(50) NOT NULL,
	ActivityID Int NOT NULL,
	CONSTRAINT CLUB_PK PRIMARY KEY(ClubID),
	CONSTRAINT CLUB_ADVISORID_FK FOREIGN KEY (AdvisorID) REFERENCES CLADVISOR (AdvisorID),
	CONSTRAINT CLUB_ACTIVITYID_FK FOREIGN KEY (ActivityID) REFERENCES CLACTIVITIES (ActivityID)
	);

CREATE TABLE FUNDRAISER ( 
	FundraiserID Int NOT NULL,
	ClubID Int NOT NULL,
	SponsorID Int NOT NULL, 
	FundTypeID Int NOT NULL, 
	Date Date NOT NULL,
	CONSTRAINT FUNDRAISER_PK PRIMARY KEY(FundraiserID),
	CONSTRAINT FUNDRAISER_CLUBID_FK FOREIGN KEY (ClubID) REFERENCES CLUB (ClubID),
	CONSTRAINT FUNDRAISER_SPONSORID_FK FOREIGN KEY (SponsorID) REFERENCES SPONSOR (SponsorID), 
	CONSTRAINT FUNDRAISER_FUNDTYPEID_FK FOREIGN KEY (FundTypeID) REFERENCES FUNDRAISERTYPE (FundTypeID)
	);

CREATE TABLE STUDENTMEMBER ( 
	MembershipID Int NOT NULL,
	ClubID Int NOT NULL,
	StudentID Int NOT NULL, 
	StartDate Date NOT NULL, 
	EndDate Date,
	CONSTRAINT STUDENTMEMBER_PK PRIMARY KEY(MembershipID),
	CONSTRAINT STUDENTMEMBER_CLUBID_FK FOREIGN KEY (ClubID) REFERENCES CLUB (ClubID),
	CONSTRAINT STUDENTMEMBER_STUDENTID_FK FOREIGN KEY (StudentID) REFERENCES STUDENT (StudentID) 
	);

